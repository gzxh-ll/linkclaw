{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "OpenClaw Manager 默认权限配置",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "shell:allow-execute",
    "shell:allow-spawn",
    "shell:allow-stdin-write",
    "shell:allow-kill",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write",
    "process:default",
    "notification:default"
  ]
}
